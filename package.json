{
	"name": "node-app",
	"description": "hello jenkins test app",
	"version": "0.0.1",
	"private": true,
	"dependencies": {
		"express": "3.12.0",
		"pm2": "~1.1.3"
	},
	"devDependencies": {
		"mocha": "1.20.1",
		"supertest": "0.13.0"
	},
	"scripts": {
		"start": "./node_modules/pm2/bin/pm2 start index.js --name node-app",
		"stop": "./node_modules/pm2/bin/pm2 stop node-app",
		"reload": "./node_modules/pm2/bin/pm2 gracefulReload app",
		"test": "./node_modules/.bin/_mocha ./test/test.js"
	}
}
